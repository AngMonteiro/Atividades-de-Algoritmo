algoritmo "semnome"

var
   tc: literal
   ct: inteiro
   valor: real
   cheio: real
inicio
      escreval("Digite o tipo do seu carro(G = gasolina ou A = álcool):")
      leia(tc)
      escreval("Digite a capacidade em litros do seu tanque:")
      leia(ct)
      se(tc = "G")entao
            escreval("Seu carro é tipo Gasolina.Digite quanto está o preço da Gasolina em litros:")
            leia(valor)
      senao
            escreval("Seu carro é tipo Álcool.Digite quanto está o preço do Álcool em litros:")
            leia(valor)
      fimse
      cheio <- ct*valor
      escreva("O preço para se encher o tanque de compustivel é:", cheio," reais")

      
fimalgoritmo
