algoritmo "semnome"

var
   n,maior:inteiro
inicio
      escreval("O programa ira informar o maior número digitado")
      escreval("Digite 0 para terminar a repetição")
      escreval
      n<-1
      enquanto n <> 0 faca
               escreval("Digite um número:")
               leia(n)
               
               se (n > maior) entao
                  maior<-n
               fimse
      fimenquanto
      escreval("O maior número digitado foi: ",maior)
fimalgoritmo
